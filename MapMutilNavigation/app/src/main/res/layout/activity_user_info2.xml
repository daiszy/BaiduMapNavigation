<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_user_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.gaoxixi.mapmutilnavigation.Activity.UserInfoActivity">

    <TableLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_margin="10dp"
        android:padding="10dp">
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <TextView
                android:text="昵称:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_nickName"
                android:layout_width="match_parent"
                android:layout_marginRight="10dp"
                android:text="测试"
                android:textSize="20dp"/>
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="7dp">
            <TextView
                android:text="姓名:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_name"
                android:text="高"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_marginRight="10dp"/>
        </TableRow>>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="7dp">
            <TextView
                android:text="性别:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_sex"
                android:text="女"
                android:textSize="20dp"/>

        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="7dp">
            <TextView
                android:text="手机号:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_telphone"
                android:text="18829027144"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_marginRight="10dp"/>
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="7dp">
            <TextView
                android:text="所在城市:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_ofCity"
                android:text="西安"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_marginRight="10dp"/>
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="7dp">
            <TextView
                android:text="状态:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_status"
                android:text="空闲"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_marginRight="10dp"/>
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="7dp">
            <TextView
                android:text="等级:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_grade"
                android:text="1"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_marginRight="10dp"/>
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="7dp">
            <TextView
                android:text="已接订单数:"
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                />
            <TextView
                android:id="@+id/info_ordersNum"
                android:text="0"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_marginRight="10dp"/>
        </TableRow>
    </TableLayout>>
</RelativeLayout>
